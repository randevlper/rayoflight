<Window x:Class="Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Editor"
        mc:Ignorable="d"
        Title="Rayoflight Editor" Height="150" Width="400">
    <StackPanel>
        <Button x:Name="Build" Click="Build_Click" Content="Build"/>
        <Button x:Name="Start" Click="Start_Click" Content="Start"/>
        <TextBlock Text="Build Status:"/>
        <TextBox x:Name="BuildStatus" IsEnabled="False"/>
    </StackPanel>
</Window>
